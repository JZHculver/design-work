{
  "name": "snmobile_ui",
  "version": "0.1.0",
  "description": "web app for snmobile ui.",
  "main": "bin/server/index.js",
  "repository": "git@git.yonyou.com:xiaojing/snmobile.git",
  "author": "fangqg@yonyou.com",
  "license": "ISC",
  "scripts": {
    "start": "cross-env SERVER_PORT=3003 SRV_URL=http://uretailtestserver.yonyoucloud.com/uretail NODE_ENV=production node bin/server/index.js",
    "debug:server": "cross-env BABEL_ENV=production NODE_ENV=development SRV_URL=http://uretailtestserver.yonyoucloud.com/uretail SCRIPT_PORT=3005 nodemon -w src/server -w src/common --exec babel-node --debug src/server/app.jsx",
    "debug:client": "cross-env BABEL_ENV=development SCRIPT_PORT=3005 webpack-dev-server --progress --colors  --config webpack.dev.config.js",
    "build:dll:dev": "cross-env NODE_ENV=development webpack --config webpack.dll.config.js",
    "build:dll:prod": "cross-env NODE_ENV=production webpack --config webpack.dll.config.js",
    "build:dll": "npm run build:dll:dev && npm run build:dll:prod",
    "build:server": "cross-env BABEL_ENV=production babel src -d bin --ignore client && echo '后端程序：编译完成'",
    "build:client": "cross-env BABEL_ENV=production webpack --config webpack.prod.config.js --colors --progress && echo '前端程序：编译完成'",
    "build": "npm run build:server && npm run build:client",
    "package": "cross-env BABEL_ENV=production webpack --config webpack.package.config.js --colors --progress && echo '前端程序：编译完成'",
    "prebuild": "npm run clean",
    "clean": "rimraf ./bin"
  },
  "dependencies": {
    "antd": "^2.12.1",
    "antd-mobile": "1.4.2",
    "classnames": "^2.2.5",
    "cookies-js": "^1.2.3",
    "echarts": "^4.2.1",
    "echarts-for-react": "^2.0.15-beta.0",
    "es6-promise": "^4.1.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "keymirror": "^0.1.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.4",
    "log4js": "^1.0.1",
    "moment": "^2.17.1",
    "prop-types": "^15.5.10",
    "rc-form": "1.4.3",
    "react": "^15.4.2",
    "react-amap": "^1.0.1",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "warning": "^3.0.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.3.3",
    "postcss-pxtorem": "^4.0.1",
    "rimraf": "^2.5.4",
    "rmc-list-view": "0.8.3",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "^0.3.1",
    "url-loader": "^0.5.8",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
